clear
echo Iris Contour
python ./cis.py plot rain:"/home/shared/NetCDF Files/xglnwa.pm.k8dec-k9nov.col.tm.nc" --type contour --cmap RdBu --xlabel "Overidden X Label" --title "Overidded Title" --height 5 --width 5 --xrange 15:45:20 --cbarorient vertical --fontsize 10 --output /home/shared/testplots/iriscontour
echo Iris Contourf
python ./cis.py plot rain:"/home/shared/NetCDF Files/xglnwa.pm.k8dec-k9nov.col.tm.nc" --type contourf --ylabel "Overidden Y Label" --height 5 --width 10 --yrange 15:45:10 --nocolourbar --output /home/shared/testplots/iriscontourf
echo Iris Heatmap
python ./cis.py plot rain:"/home/shared/NetCDF Files/xglnwa.pm.k8dec-k9nov.col.tm.nc" --type heatmap --cmap RdBu --ylabel OveriddenY --title OveriddenTitle --valrange :0.000135 --fontsize 10 --height 3.5 --cbarorient horizontal --output /home/shared/testplots/irisheatmap
echo Iris one line
python ./cis.py plot rain:"/home/shared/NetCDF Files/xglnwa.pm.k8dec-k9nov.vprof.tm.nc"::dashed:overiddenlabel --xlabel overiddenxlabel --title overiddentitle --itemwidth 4 --height 5 --width 10 --logy --grid --output /home/shared/testplots/irisoneline
echo Iris many lines
python ./cis.py plot rain:"/home/shared/NetCDF Files/xglnwa.pm.k8dec-k9nov.vprof.tm.nc":red snow:"/home/shared/NetCDF Files/xglnwa.pm.k8dec-k9nov.vprof.tm.nc"::dashed:overiddenlabel2 --xlabel overiddenxlabel --ylabel overiddenylabel --itemwidth 5 --width 10 --logx --grid --xrange 0.00000000001: --yrange :50 --output /home/shared/testplots/irismanylines
echo Iris Scatter 2D
python ./cis.py plot rain:"/home/shared/NetCDF Files/xglnwa.pm.k8dec-k9nov.vprof.tm.nc"::^: --type scatter --title overiddentitle --fontsize 10 --width 7 --itemwidth 300 --xrange=-0.0005:0.0005 --output /home/shared/testplots/irisscatter2d
echo Iris Scatter 3D
python ./cis.py plot rain:"/home/shared/NetCDF Files/xglnwa.pm.k8dec-k9nov.col.tm.nc" snow:"/home/shared/NetCDF Files/xglnwa.pm.k8dec-k9nov.col.tm.nc":::snowlabel --type scatter --cmap RdBu --fontsize 7 --cbarorient vertical --grid --yrange 0:90 --valrange 0: --cbarorient vertical --output /home/shared/testplots/irisscatter3d
echo Iris Scatter Overlay
python ./cis.py plot rain:"/home/shared/NetCDF Files/xglnwa.pm.k8dec-k9nov.col.tm.nc" snow:"/home/shared/NetCDF Files/xglnwa.pm.k8dec-k9nov.col.tm.nc"::^:snowlabel --type scatteroverlay --xlabel overiddenxlabel --height 10 --width 12 --xrange 0:200:10 --cbarorient horizontal --yrange 0:90 --valrange 0: --cbarorient horizontal --output /home/shared/testplots/irisscatteroverlay
